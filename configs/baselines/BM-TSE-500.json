        "optimizer":"torch.optim.Adam",
        "optimizer_params":{
            "lr":1e-3
        }
    },
    "grad_clip": 1,
    "pl_logger": "src.pl_modules.ts_hear_binaural_TSE_pl_module_part.TSHearLogger",
