{
    "pl_module": "src.pl_modules.ts_hear_binaural_TSE_pl_module_part.TSHearPLModule",
    "pl_module_args": {
        "model": "src.models.tfgridnet_TSE_causal_torch_stft_layernorm.net.Net",
        "init_ckpt": null,
